<div id="<%= dom_id event %>">
  <p>
    <strong>Id:</strong>
    <%= event.id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= event.date %>
  </p>

  <p>
    <strong>Datetime:</strong>
    <%= event.datetime %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= event.location %>
  </p>

  <p>
    <strong>Attendees:</strong>
    <%= event.attendees %>
  </p>

</div>
